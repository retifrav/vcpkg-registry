{
    "name": "implot",
    "version": "0.15.0",
    "description": "Immediate mode plotting",
    "homepage": "https://github.com/epezent/implot",
    "dependencies":
    [
        {
            "name": "vcpkg-cmake",
            "host": true
        },
        {
            "name": "vcpkg-cmake-config",
            "host": true
        },
        {
            "name": "decovar-vcpkg-cmake",
            "host": true
        },
        {
            "name": "dearimgui",
            "version>=": "1.89.8",
            "features":
            [
                "indices-x32",
                "math-operators"
            ]
        }
    ],
    "default-features":
    [
        "with-internal"
    ],
    "features":
    {
        "with-internal":
        {
            "description": "Using internal header(s)"
        }
    }
}
