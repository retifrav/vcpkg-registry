{
    "name": "proj",
    "version": "9.6.2",
    "description": "Cartographic projections and coordinate transformations",
    "homepage": "https://proj.org/",
    "license": "MIT",
    "dependencies":
    [
        {
            "name": "vcpkg-cmake",
            "host": true
        },
        {
            "name": "vcpkg-cmake-config",
            "host": true
        },
        "json-nlohmann",
        "sqlite",
        {
            "name": "sqlite",
            "host": true,
            "features":
            [
                "tool"
            ]
        }
    ],
    "default-features":
    [
        "tiff"
    ],
    "features":
    {
        "curl":
        {
            "description": "Enable network capability with cURL",
            "dependencies":
            [
                "curl"
            ]
        },
        "embed-resource-files":
        {
            "description": "Embedding proj.db (and some other resources?) into the library"
        },
        "tiff":
        {
            "description": "Enable TIFF support",
            "dependencies":
            [
                "tiff"
            ]
        }
    }
}
