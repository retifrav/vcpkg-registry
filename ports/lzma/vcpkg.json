{
    "name": "lzma",
    "version": "25.0.0",
    "description": "LZMA provides a high compression ratio and fast decompression",
    "homepage": "https://7-zip.org/sdk.html",
    "dependencies":
    [
        {
            "name": "vcpkg-cmake",
            "host": true
        },
        {
            "name": "vcpkg-cmake-config",
            "host": true
        },
        {
            "name": "decovar-vcpkg-cmake",
            "host": true
        }
    ],
    "default-features":
    [
        "enable-avx2"
    ],
    "features":
    {
        "enable-avx2":
        {
            "description": "Enable AVX2 intrinsics"
        }
    }
}
